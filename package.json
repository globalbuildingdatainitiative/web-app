    "@mantine/dropzone": "^7.11.0",
    "@mantine/form": "^7.11.0",
    "@mantine/hooks": "^7.11.0",
    "@mantine/notifications": "^7.13.2",
    "@tabler/icons-react": "^3.2.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.10",
    "lcax": "^2.6.1",
    "leaflet": "^1.9.4",
